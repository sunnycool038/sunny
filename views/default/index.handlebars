<div class="row">
    <div class="col-md-8">
      <h1 class="my-4">Page heading
        <small>Secodary text</small>
      </h1>
      {{#each posts}}
      <div class="card md-4">
        <img class="card-img-top" src="https://placeimg.com/750/300/tech" alt="Card image cap">
        <div class="card-body">
          <h2 class="card-title">{{title}}</title></h2>
          <p class="card-text">{{description}}</p>
          <a href="/post/{{id}}" class="btn btn-primary">Read more &rarr;</a>
        </div>
        <div class="card-footer text-mooted">
          Posted on {{CreationDate}} by
          <a href="#">Admin</a>
        </div>
      </div>
      {{/each}}
      <ul class="pagination justify-content-center mb-4">
        <li class="page-item">
          <a class="page-link" href="#">&larr;Older</a>
        </li>
        <li class="page-item disabled">
          <a class="page-link" href="#">Newer &larr;</a>
        </li>
      </ul>
    </div>
    <div class="col-md-4">
      <div class="card-md-4">
        <h5 class="card-header">Search</h5>
        <div class="card-body">
          <div class="input-group">
            <input type="text" class="form-control" placeholder="Search for...">
            <span class="input-group-btn">
              <button class="btn btn-secondary" type="button">Go!</button>
            </span>
          </div>
        </div>
      </div>
      <div class="card my-4">
        <h5 class="card-header">Categories</h5>
        <div class="card-body">
          <div class="row">
            <div class="col-lg-6">
              <ul class="list-unstyled mb-0">
                {{#each categories}}
                <li>
                  <a href="#">{{title}}</a>
                </li>
                {{/each}}
              </ul>
            </div>
          </div>
        </div>
      </div>
      <div class="card my-4">
        <h5 class="card-header">Side Widget</h5>
        <div class="card-body">
          This is a very good job
        </div>
      </div>
    </div>
  </div>



